# Configurações de conexão do banco de dados
# Copie este arquivo para .env e preencha com suas credenciais reais

# DSN (Data Source Name) - Nome da fonte de dados ODBC
DSN=seu_dsn_aqui

# Credenciais do banco
DB_USER=seu_usuario
DB_PASSWORD=sua_senha

# Configurações opcionais
DB_HOST=localhost
DB_PORT=3050
DB_DATABASE=seu_banco.fdb

# Configurações de teste
TEST_SQL=SELECT FIRST 10 * FROM RDB$DATABASE